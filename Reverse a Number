import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N= sc.nextInt();
        
        int reversedNumber = 0;
        while (N > 0) {
            int digit = N % 10;
            reversedNumber = reversedNumber * 10 + digit;
            N = N / 10;
        }
        
        System.out.println(reversedNumber);
        
    }
}





