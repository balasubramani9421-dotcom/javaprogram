import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int T = sc.nextInt(); 

        for (int i = 0; i < T; i++) {
            int N = sc.nextInt();

            for (int j = 1; j <= N; j++) {
                System.out.print(j);
                if (j < N) System.out.print(" ");
            }
            System.out.println(); 
        }

        sc.close();
    }
}
