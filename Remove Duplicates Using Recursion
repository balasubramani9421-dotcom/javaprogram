import java.io.*;
import java.util.*;

public class Solution {
    static String removeDuplicates(String str, String result) {
    if (str.length() == 0) {
        return result;
    }
    char ch = str.charAt(0);
    if (result.indexOf(ch) == -1) {
        result += ch;
    }
    return removeDuplicates(str.substring(1), result);
}
}


    
